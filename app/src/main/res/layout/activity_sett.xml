<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:orientation="vertical">

    <LinearLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_44"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/sett_back"
            android:layout_width="@dimen/xdp_13"
            android:layout_height="@dimen/xdp_22"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/xdp_12"
            android:src="@mipmap/fanhui" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="设置"
            android:textColor="@color/black"
            android:textSize="@dimen/xsp_17" />
    </LinearLayout>

    <LinearLayout
        android:background="@color/white"

        android:id="@+id/sett_namelin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_44"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/xdp_15"
            android:text="消息推送通知"
            android:textColor="@color/black"
            android:textSize="@dimen/xsp_15" />

        <TextView
            android:id="@+id/sett_name"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/xdp_15"
            android:layout_weight="1"
            android:gravity="right|center"
            android:textColor="@color/mine_unselect"
            android:textSize="@dimen/xsp_13" />

        <ImageView
            android:id="@+id/setimg"
            android:layout_width="@dimen/xdp_10"
            android:layout_height="@dimen/xdp_17"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/xdp_13"
            android:src="@mipmap/gengduo"
            />
    </LinearLayout>
    <TextView
        android:id="@+id/set_noti"
        android:visibility="gone"
        android:layout_marginLeft="@dimen/xdp_12"
        android:layout_marginRight="@dimen/xdp_12"
        android:layout_width="match_parent"
        android:textSize="@dimen/xsp_11"
        android:text="建议您开启消息通知,开启后可及时收到关于商品优惠、订单物流等重要通知"
        android:layout_height="@dimen/xdp_44" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_1"
        android:layout_marginLeft="@dimen/xdp_13"
        android:layout_marginRight="@dimen/xdp_13"
        android:background="@color/background" />

    <LinearLayout
        android:background="@color/white"

        android:id="@+id/sett_lin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_44"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/xdp_15"
            android:text="实名认证"

            android:textColor="@color/black"
            android:textSize="@dimen/xsp_15" />

        <TextView
            android:id="@+id/sett_autonmy"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/xdp_15"
            android:layout_weight="1"
            android:ellipsize="end"
            android:gravity="right|center"
            android:maxLength="7"
            android:text="未认证"
            android:singleLine="true"
            android:textColor="@color/mine_unselect"
            android:textSize="@dimen/xsp_13" />

        <ImageView
            android:layout_width="@dimen/xdp_10"
            android:layout_height="@dimen/xdp_17"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/xdp_13"
            android:src="@mipmap/gengduo"


            />
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_1"
        android:layout_marginLeft="@dimen/xdp_13"
        android:layout_marginRight="@dimen/xdp_13"
        android:background="@color/background" />

    <LinearLayout
        android:background="@color/white"

        android:id="@+id/sett_signlin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_44"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/xdp_15"
            android:text="个人信息设置"

            android:textColor="@color/black"
            android:textSize="@dimen/xsp_15" />

        <TextView
            android:id="@+id/sett_sign"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/xdp_15"
            android:layout_weight="1"
            android:ellipsize="end"
            android:gravity="right|center"
            android:maxLength="7"
            android:singleLine="true"
            android:textColor="@color/mine_unselect"
            android:textSize="@dimen/xsp_13" />

        <ImageView
            android:layout_width="@dimen/xdp_10"
            android:layout_height="@dimen/xdp_17"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/xdp_13"
            android:src="@mipmap/gengduo"


            />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_1"
        android:layout_marginLeft="@dimen/xdp_13"
        android:layout_marginRight="@dimen/xdp_13"
        android:background="@color/background" />

    <LinearLayout
        android:background="@color/white"
        android:id="@+id/sett_sexlin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_44"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/xdp_15"
            android:text="收货地址"
            android:textColor="@color/black"
            android:textSize="@dimen/xsp_15" />

        <TextView
            android:id="@+id/sett_sex"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/xdp_15"
            android:layout_weight="1"
            android:gravity="right|center"
            android:textColor="@color/mine_unselect"
            android:textSize="@dimen/xsp_13" />

        <ImageView
            android:layout_width="@dimen/xdp_10"
            android:layout_height="@dimen/xdp_17"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/xdp_13"
            android:src="@mipmap/gengduo"


            />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_1"
        android:layout_marginLeft="@dimen/xdp_13"
        android:layout_marginRight="@dimen/xdp_13"
        android:background="@color/background" />

    <LinearLayout
        android:background="@color/white"
        android:id="@+id/sett_birthlin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_44"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/xdp_15"
            android:text="账号与安全"
            android:textColor="@color/black"
            android:textSize="@dimen/xsp_15" />

        <TextView
            android:id="@+id/sett_birth"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/xdp_15"
            android:layout_weight="1"
            android:gravity="right|center"
            android:textColor="@color/mine_unselect"
            android:textSize="@dimen/xsp_13" />

        <ImageView
            android:layout_width="@dimen/xdp_10"
            android:layout_height="@dimen/xdp_17"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/xdp_13"
            android:src="@mipmap/gengduo"


            />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_1"
        android:layout_marginLeft="@dimen/xdp_13"
        android:layout_marginRight="@dimen/xdp_13"
        android:visibility="visible"
        android:background="@color/background" />

    <LinearLayout
        android:background="@color/white"
        android:id="@+id/sett_heighlin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_44"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/xdp_15"
            android:text="清除缓存"
            android:textColor="@color/black"
            android:textSize="@dimen/xsp_15" />

        <TextView
            android:id="@+id/sett_heigh"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/xdp_15"
            android:layout_weight="1"
            android:gravity="right|center"
            android:textColor="@color/mine_unselect"
            android:textSize="@dimen/xsp_13" />

        <ImageView
            android:layout_width="@dimen/xdp_10"
            android:layout_height="@dimen/xdp_17"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/xdp_13"
            android:src="@mipmap/gengduo"


            />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_1"
        android:visibility="visible"
        android:layout_marginLeft="@dimen/xdp_13"
        android:layout_marginRight="@dimen/xdp_13"
        android:background="@color/background" />

    <LinearLayout
        android:background="@color/white"
        android:id="@+id/sett_bindslin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_44"
        android:orientation="horizontal"
        android:visibility="visible">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"

            android:layout_marginLeft="@dimen/xdp_15"
            android:text="绑定公众号"
            android:textColor="@color/black"
            android:textSize="@dimen/xsp_15" />

        <TextView
            android:id="@+id/sett_binds"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/xdp_15"
            android:layout_weight="1"
            android:gravity="right|center"
            android:textColor="@color/mine_unselect"
            android:textSize="@dimen/xsp_13" />

        <ImageView
            android:layout_width="@dimen/xdp_10"
            android:layout_height="@dimen/xdp_17"
            android:layout_gravity="center"

            android:layout_marginRight="@dimen/xdp_13"
            android:src="@mipmap/gengduo"


            />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_1"
        android:layout_marginLeft="@dimen/xdp_13"
        android:layout_marginRight="@dimen/xdp_13"
        android:background="@color/background" />

    <LinearLayout
        android:background="@color/white"
        android:id="@+id/sett_aboutlin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_44"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/xdp_15"
            android:text="关于我们"
            android:textColor="@color/black"
            android:textSize="@dimen/xsp_15" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/xdp_15"
            android:layout_weight="1"
            android:gravity="right|center"
            android:textColor="@color/mine_unselect"
            android:textSize="@dimen/xsp_13" />

        <ImageView
            android:layout_width="@dimen/xdp_10"
            android:layout_height="@dimen/xdp_17"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/xdp_13"
            android:src="@mipmap/gengduo"


            />
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_1"
        android:layout_marginLeft="@dimen/xdp_13"
        android:layout_marginRight="@dimen/xdp_13"
        android:background="@color/background" />
    <LinearLayout
        android:background="@color/white"
        android:id="@+id/sett_exitlin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xdp_44"
        android:layout_gravity="center"
        android:orientation="horizontal"
        android:visibility="visible">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/xdp_15"
            android:text="退出登录"
            android:textColor="@color/black"
            android:textSize="@dimen/xsp_15" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/xdp_15"
            android:layout_weight="1"
            android:gravity="right|center"
            android:textColor="@color/mine_unselect"
            android:textSize="@dimen/xsp_13" />

        <ImageView
            android:layout_width="@dimen/xdp_10"
            android:layout_height="@dimen/xdp_17"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/xdp_13"
            android:src="@mipmap/gengduo"


            />
    </LinearLayout>


</LinearLayout>